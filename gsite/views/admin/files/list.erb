<a href="/admin/files/new" class="btn btn-default" style="width: 100%; margin-bottom: 20px;">Создать файл</a>

<table class="table text-center">
	<tr>
		<th class="text-center">Имя</th>
		<th class="text-center">Управляющие ссылки</th>
	</tr>
	<% @files.each do |file| %>
	<tr>
		<td><%= file[:name] %></td>
		<td>
			<a href="/admin/files/<%= file[:_id] %>/delete">Удалить</a>
		</td>
	</tr>
	<% end %>
</table>
